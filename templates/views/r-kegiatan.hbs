<!DOCTYPE html>

<html>

<head>

</head>

<body>
    {{>header}}
    <br />
    <br />
    <br />
    <div class="ui container">
        <div class="ui segment">
            <div class="ui breadcrumb">
                <a href="#" class="section">Scrap</a>
                <i class="right chevron icon divider"></i>
                <div class="active section">RKPD Kegiatan</div>
            </div>
        </div>
        <div class="ui segment">
            <table class="small ui yellow celled table">
                <thead>
                    <tr>
                        <th>Kode</th>
                        <th>Kegiatan</th>
                        <th>Action</th>
                    </tr>
                </thead>
                <tbody>
                    {{#each result}}
                    <tr>
                        <td class="ui center aligned">{{this.kode_giat}}</td>
                        <td><a href="#">{{this.nama_giat}}</a></td>
                        <td>
                            <a href="#" class="ui success label" title="Detail"><i class="icon file alternate"></i></a>
                        </td>
                    </tr>
                    {{/each}}
                </tbody>
            </table>
        </div>
    </div>

    {{>footer}}

</body>